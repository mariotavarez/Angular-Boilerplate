{
	"name": "Hexagonal Boilerplate",
	"version": "0.0.2",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test:coverage": "jest --coverage --watchAll=true",
		"test:ci": "jest --ci --coverage --watchAll=false || exit 1",
		"version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
		"lint": "eslint '*/**/*.{js,ts,tsx}'",
		"prettier-write": "prettier --write",
		"prettier-check": "prettier --check",
		"prettier-write-all": "prettier --write */**/*.{ts,json}",
		"prettier-check-all": "prettier --check */**/*.{ts,json}",
		"docs:json": "compodoc -p ./tsconfig.json -e json -d .",
		"storybook": "npm run docs:json && start-storybook -p 6006",
		"build-storybook": "npm run docs:json && build-storybook",
		"prepare": "husky install",
		"release": "standard-version --releaseCommitMessageFormat=\"chore(release): {{currentTag}} [ci skip]\"",
		"release:pre": "standard-version --prerelease --releaseCommitMessageFormat=\"chore(release): {{currentTag}} [ci skip]\"",
		"release:alpha": "standard-version --prerelease alpha"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^15.0.3",
		"@angular/common": "^15.0.0",
		"@angular/compiler": "^15.0.0",
		"@angular/core": "^15.0.0",
		"@angular/forms": "^15.0.0",
		"@angular/platform-browser": "^15.0.0",
		"@angular/platform-browser-dynamic": "^15.0.0",
		"@angular/router": "^15.0.0",
		"axios": "^1.2.1",
		"bootstrap": "^5.2.3",
		"dayjs": "^1.11.7",
		"maxill-ui": "^0.0.5",
		"ngx-toastr": "^16.0.1",
		"rxjs": "~7.5.0",
		"tslib": "^2.3.0",
		"zone.js": "~0.12.0"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^15.0.3",
		"@angular/cli": "~15.0.3",
		"@angular/compiler-cli": "^15.0.0",
		"@babel/core": "^7.20.5",
		"@commitlint/cli": "^17.3.0",
		"@commitlint/config-conventional": "^17.3.0",
		"@compodoc/compodoc": "^1.1.19",
		"@storybook/addon-actions": "^6.5.14",
		"@storybook/addon-essentials": "^6.5.14",
		"@storybook/addon-interactions": "^6.5.14",
		"@storybook/addon-links": "^6.5.14",
		"@storybook/angular": "^6.5.14",
		"@storybook/builder-webpack5": "^6.5.14",
		"@storybook/manager-webpack5": "^6.5.14",
		"@storybook/testing-library": "^0.0.13",
		"@types/jest": "^29.2.4",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.46.0",
		"babel-loader": "^8.3.0",
		"eslint": "^8.29.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-hexagonal-architecture": "^1.0.3",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-storybook": "^0.6.8",
		"husky": "^8.0.2",
		"jest": "^29.3.1",
		"jest-preset-angular": "^12.2.3",
		"lint-staged": "^13.1.0",
		"prettier": "^2.8.1",
		"prettier-eslint": "^15.0.1",
		"standard-version": "^9.5.0",
		"typescript": "^4.8.4"
	},
	"jest": {
		"preset": "jest-preset-angular",
		"setupFilesAfterEnv": [
			"<rootDir>/setup-jest.ts"
		],
		"globalSetup": "jest-preset-angular/global-setup"
	}
}
